package Enum;

public enum DiaSemana {
	
	SEGUNDA,
	TERÇA,
	QUARTA,
	QUINTA,
	SEXTA,
	SÁBADO,
	DOMINGO;

}

**************************************

package Enum;

public class PrincipalEnum {

	public static void main(String[] args) {
        
		DiaSemana dia = DiaSemana.SEGUNDA;
		
		System.out.println(dia);
	}

}
